{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "starter for webpack React",
  "main": "server/index.js",
  "author": "Pano",
  "license": "ISC",
  "scripts": {
    "client": "webpack-dev-server --hot --inline",
    "dev": "node server & webpack --watch",
    "server": "node server",
    "start": "webpack && node server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass --watch client/sass -o public/css"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "dependencies": {
    "express": "^4.17.1",
    "superagent": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "cors": "^2.8.5",
    "jsdom": "^15.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "node-sass": "^4.12.0",
    "sass": "^1.22.1",
    "sass-loader": "^7.1.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pano-skylakis/boilerplate-react-webpack.git"
  },
  "bugs": {
    "url": "https://github.com/pano-skylakis/boilerplate-react-webpack/issues"
  },
  "homepage": "https://github.com/pano-skylakis/boilerplate-react-webpack#readme"
}
